{
  "name": "studio-pandora-api",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/src/server.js",
    "dev": "ts-node-dev src/server.ts",
    "test": "vitest --config vitest.config.ts",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "typeorm": "ts-node node_modules/typeorm/cli.js",
    "migration:create": "npm run typeorm migration:create src/migrations/Default",
    "migration:generate": "npm run typeorm migration:generate src/migrations/Default -- -d src/utils/data-source-migration-generate.ts",
    "migration:run": "npm run typeorm migration:run -- -d src/utils/data-source.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.2",
    "vitest": "^0.29.8"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.1",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.2",
    "pg": "^8.10.0",
    "redis": "^4.6.5",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.14",
    "uuidv4": "^6.2.13",
    "yup": "^1.0.2"
  }
}
